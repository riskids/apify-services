# Apify Configuration
APIFY_TOKEN_PRIMARY=your-primary-token
APIFY_TOKEN_FILE_PATH=./config/apify-token.txt

# Server Configuration
PORT=3000
NODE_ENV=development

# Output Configuration
OUTPUT_DIR=./output
ENABLE_DATABASE_STORAGE=false
DATABASE_URL=

# Queue Configuration
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT=3600000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_DIR=./logs
